<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/biomenu"
    android:background="@color/white"
    tools:context=".ui.home.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="270dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="698dp"
                android:layout_height="310dp"
                android:src="@drawable/backgroundbio"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.501"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="-42dp" />

            <ImageView
                android:id="@+id/biomen"
                android:layout_width="262dp"
                android:layout_height="241dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/biovisionlogo1" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="550dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

            <ImageView
                android:id="@+id/detection"
                android:layout_width="157dp"
                android:layout_height="187dp"
                android:layout_marginBottom="280dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/boxblue1" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="157dp"
                android:layout_height="175dp"
                app:layout_constraintBottom_toBottomOf="@+id/detection"
                app:layout_constraintEnd_toEndOf="@+id/detection"
                app:layout_constraintStart_toStartOf="@+id/detection"
                app:layout_constraintTop_toTopOf="@+id/detection"
                app:srcCompat="@drawable/frogbutton" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="160dp"
                android:layout_height="35dp"
                android:layout_marginBottom="8dp"
                android:background="@color/white"
                android:fontFamily="arial"
                android:text="FROG"
                android:textAlignment="center"
                android:textColor="@color/merigold"
                android:textSize="22dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/detection"
                app:layout_constraintEnd_toEndOf="@+id/detection"
                app:layout_constraintHorizontal_bias="0.423"
                app:layout_constraintStart_toStartOf="@+id/detection" />

            <ImageView
                android:id="@+id/fish"
                android:layout_width="157dp"
                android:layout_height="187dp"
                app:layout_constraintBottom_toBottomOf="@+id/detection"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.933"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/detection"
                app:srcCompat="@drawable/boxblue1" />

            <ImageView
                android:id="@+id/tilapiacam"
                android:layout_width="168dp"
                android:layout_height="143dp"

                android:layout_marginStart="8dp"

                app:layout_constraintBottom_toBottomOf="@+id/fish"
                app:layout_constraintStart_toStartOf="@+id/fish"
                app:layout_constraintTop_toTopOf="@+id/fish"
                app:layout_constraintVertical_bias="0.305"
                app:srcCompat="@drawable/fishbutton" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="160dp"
                android:layout_height="35dp"
                android:layout_marginBottom="8dp"
                android:background="@color/white"
                android:fontFamily="arial"
                android:text="TILAPIA"
                android:textAlignment="center"
                android:textColor="@color/merigold"
                android:textSize="22dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/fish"
                app:layout_constraintEnd_toEndOf="@+id/fish"
                app:layout_constraintHorizontal_bias="0.423"
                app:layout_constraintStart_toStartOf="@+id/fish" />

            <ImageView
                android:id="@+id/chickencam"
                android:layout_width="157dp"
                android:layout_height="187dp"
                android:layout_marginBottom="28dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.074"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:layout_constraintVertical_bias="0.369"
                app:srcCompat="@drawable/boxblue1" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="160dp"
                android:layout_height="35dp"
                android:layout_marginBottom="8dp"
                android:background="@color/white"
                android:fontFamily="arial"
                android:text="CHICKEN"
                android:textAlignment="center"
                android:textColor="@color/merigold"
                android:textSize="22dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/chickencam"
                app:layout_constraintEnd_toEndOf="@+id/chickencam"
                app:layout_constraintHorizontal_bias="0.423"
                app:layout_constraintStart_toStartOf="@+id/chickencam" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="216dp"
                android:layout_height="149dp"
                app:layout_constraintEnd_toEndOf="@+id/chickencam"
                app:layout_constraintStart_toStartOf="@+id/chickencam"
                app:layout_constraintTop_toTopOf="@+id/chickencam"
                app:srcCompat="@drawable/chickenbutton" />

            <ImageView
                android:id="@+id/Guide"
                android:layout_width="157dp"
                android:layout_height="187dp"
                app:layout_constraintBottom_toBottomOf="@+id/chickencam"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.933"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/chickencam"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/boxblue1" />

            <ImageView
                android:id="@+id/dissected"
                android:layout_width="159dp"
                android:layout_height="123dp"
                app:layout_constraintBottom_toBottomOf="@+id/Guide"
                app:layout_constraintEnd_toEndOf="@+id/Guide"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Guide"
                app:layout_constraintTop_toTopOf="@+id/Guide"
                app:layout_constraintVertical_bias="0.125"
                app:srcCompat="@drawable/dis" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="160dp"
                android:layout_height="35dp"
                android:layout_marginBottom="8dp"
                android:background="@color/white"
                android:fontFamily="arial"
                android:text="DISSECTION"
                android:textAlignment="center"
                android:textColor="@color/merigold"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/Guide"
                app:layout_constraintEnd_toEndOf="@+id/Guide"
                app:layout_constraintStart_toStartOf="@+id/Guide" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
